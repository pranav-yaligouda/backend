{
  "name": "athani-mart-backend",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "db:optimize": "ts-node src/scripts/simpleOptimization.ts",
    "db:full-optimize": "ts-node src/scripts/databaseOptimization.ts",
    "db:ensure-indexes": "ts-node src/scripts/ensureIndexes.ts",
    "db:verify-indexes": "ts-node -e \"import('./src/config/mongoose').then(m => m.optimizeDatabase())\"",
    "db:performance-test": "ts-node src/scripts/performanceTest.ts",
    "db:cleanup": "ts-node -e \"import('./src/config/mongoose').then(m => m.cleanupDatabase())\"",
    "db:monitor": "ts-node -e \"import('./src/config/mongoose').then(m => m.monitorDatabasePerformance())\"",
    "admin:create-super": "ts-node src/scripts/createAdminUser.ts",
    "admin:test": "ts-node src/scripts/testAdminSystem.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^5.1.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@react-google-maps/api": "^2.20.7",
    "@types/cookie-parser": "^1.4.9",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "envalid": "^7.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.0.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.3.4",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "react-hook-form": "^7.59.0",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^4.6.3",
    "winston": "^3.17.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.5.5",
    "@types/swagger-ui-express": "^4.1.8",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
